mtext(paste("OUTLIERS :",paste(outliers_detection_OECD,collapse = ",")))
outliers_removed_OECD = reg1$overall_score[-which(reg1$overall_score %in% outliers_detection_OECD)]
boxplot(outliers_removed_OECD,main="BOXPLOT OF overall_score OF OECD COUNTRIES",col="mediumorchid3")


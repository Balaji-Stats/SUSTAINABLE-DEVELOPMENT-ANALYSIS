############### overall_score of SUB-SAHARAN AFRICAN COUNTRIES FOLLOWS LOGNORMAL DISTRIBUTION ###############

############### MULTI-LINEAR REGRESSION MODEL ###############
MLR_Model = lm(formula = overall_score ~ goal_1_score + 
                 goal_2_score + goal_3_score + goal_4_score +
                 goal_5_score + goal_6_score + goal_7_score + 
                 goal_8_score + goal_9_score + goal_10_score + 
                 goal_11_score + goal_12_score + goal_13_score + 
                 goal_14_score + goal_15_score + goal_16_score +
                 goal_17_score, data = data)
summary(MLR_Model)

################ PREDICTING THE FINLAND overall_score ###############
FIN_data = data.frame(goal_1_score = 99.57500, goal_2_score = 60.88675,
                      goal_3_score = 95.38638, goal_4_score = 97.16933,
                      goal_5_score = 92.11125, goal_6_score = 94.32760,
                      goal_7_score = 93.01350, goal_8_score = 86.78900,
                      goal_9_score = 95.99571, goal_10_score = 98.46850,
                      goal_11_score = 91.23375, goal_12_score = 60.05957,
                      goal_13_score = 68.793667, goal_14_score = 87.92800,
                      goal_15_score = 85.07000, goal_16_score = 92.52109,
                      goal_17_score = 75.60100)

predicted_value = predict(MLR_Model,FIN_data)
predicted_value

observed_value = data$overall_score[1]
observed_value

comparison = data.frame(observed_value,predicted_value)
comparison


############### BARGRAPH OF overall_score ###############
des_data = data[order(-data$overall_score),]
des_data

ovr1 = des_data$overall_score[1:40]
ovr1
con1 = des_data$country[1:40]
con1
data1 = data.frame(ovr1,con1)
data1
OVR_SCORE = data1$ovr1
ggplot() + geom_bar(aes(x = reorder(data1$con1,OVR_SCORE),y = OVR_SCORE,
                        fill = OVR_SCORE),stat = "identity") + 
  ggtitle("BARCHART") + theme(plot.title = element_text(hjust = 0.5)) + 
  theme(axis.text  = element_text(size = 6),
        axis.title = element_text(size = 10),
        plot.title = element_text(size = 10)) + 
  theme(axis.text.x = element_text(angle = 90,hjust = 1)) +
  xlab("COUNTRY") + ylab("OVERALL SDG SCORES") + coord_flip() +
  scale_fill_gradient(low = "blue", high = "red")


ovr2 = des_data$overall_score[41:80]
ovr2
con2 = des_data$country[41:80]
con2
data2 = data.frame(ovr2,con2)
data2

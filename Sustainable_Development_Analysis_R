############### NECESSARY LIBRARIES ###############
library(pastecs) # DESCRIPTIVE STATISTICS
library(Hmisc) # IMPUTATION 
library(fitdistrplus) # FITTING OF DISTRIBUTIONS 
library(jmv) # DESCRIPTIVE STATISTICS
library(fBasics) # BASIC STATISTICS
library(ggplot2) # DATA VISUALIZATION 
library(reshape) # DATA MANIPULATION 
library(corrplot) # CORRELATION CHART
############### DATA ###############
data = read.csv("C:\\Users\\balaj\\Documents\\DATASETS\\sustainable_development_report_2023.csv",header=T)
View(data)
head(data) 
str(data) # VARIABLE TYPES
stat.desc(data) # DESCRIPTIVE STATISTICS

############### DATA ATTRIBUTES ############### 
############### ATTRIBUTE-1 --> country_code ###############
############### ATTRIBUTE-2 --> country ###############
############### ATTRIBUTE-3 --> region ###############
pct = round(100*(table(data$region))/sum(table(data$region)))
lab = c("E.Europe & C.Asia\n","East & South Asia\n","LAC\n","\nMENA\n","\nOCEANIA\n","OECD\n","Sub-Saharan Africa\n")
pie(table(data$region),main="Pie Chart of Countries in different regions",labels=paste(lab,sep=" ",pct,"%"),col=rainbow(length(table(data$region))),clockwise = F,init.angle = 100)
############### HEATMAP ###############
new_corr_data = data[,-c(1,2,3)]

data_corr_1 = cor(new_corr_data)
data_corr_1
head(data_corr_1)

data_corr_2 = rcorr(as.matrix(new_corr_data))
data_corr_2

data_corr_2.coeff = data_corr_2$r
data_corr_2.pval = data_corr_2$P

corrplot(data_corr_2.coeff)

corr_df = melt(data_corr_2.coeff)

CORRELATION = corr_df$value
ggplot(corr_df, aes(X1, X2)) +                          
  geom_tile(aes(fill=CORRELATION)) + 
  scale_fill_gradient(low="blue4",high="orange") + 
  theme(axis.text.x =element_text(angle=90,hjust=1)) +
  ggtitle("HEATMAP") + 
  theme(plot.title = element_text(hjust=0.5)) 
############### ATTRIBUTE-4 --> overall_score ###############
head(data$overall_score)

summary(data$overall_score)

basicStats(data$overall_score)

descriptives(
  data = data,
  vars = "overall_score",
  freq = TRUE,
  hist = TRUE,
  dens = TRUE,
  bar = FALSE,
  barCounts = FALSE,
  box = FALSE,
  sd = TRUE,
  variance = TRUE,
  range = TRUE,
  se = TRUE,
  skew = TRUE,
  kurt = TRUE,
  pcEqGr = TRUE,
  pcNEqGr = 10)

boxplot(data$overall_score,main = "BOXPLOT OF overall_score",col="yellow")

fn_4 = fitdist(data$overall_score,"norm") # NORMAL DISTRIBUTION
denscomp(fn_4)
cdfcomp(fn_4)
qqcomp(fn_4)
ppcomp(fn_4)
summary(fn_4)
fln_4 = fitdist(data$overall_score,"lnorm") # LOGNORMAL DISTRIBUTION
denscomp(fln_4)
cdfcomp(fln_4)
qqcomp(fln_4)
ppcomp(fln_4)
summary(fln_4)
fw_4 = fitdist(data$overall_score,"weibull") # WEIBULL DISTRIBUTION
denscomp(fw_4)
cdfcomp(fw_4)
qqcomp(fw_4)
ppcomp(fw_4)
summary(fw_4)
fg_4 = fitdist(data$overall_score,"gamma") # GAMMA DISTRIBUTION
denscomp(fg_4)
cdfcomp(fg_4)
qqcomp(fg_4)
ppcomp(fg_4)
summary(fg_4)
fc_4 = fitdist(data$overall_score,"cauchy") # CAUCHY DISTRIBUTION
denscomp(fc_4)
cdfcomp(fc_4)
qqcomp(fc_4)
ppcomp(fc_4)
summary(fc_4)
fls_4 = fitdist(data$overall_score,"logis") # LOGISTIC DISTRIBUTION
denscomp(fls_4)
cdfcomp(fls_4)
qqcomp(fls_4)
ppcomp(fls_4)
summary(fls_4)

############### COMPARISON ###############
par(mfrow = c(2,2))
plot.legend = c("NORMAL","LOGNORMAL","WEIBULL")
par(mar=c(1,1,1,1))
denscomp(list(fn_4,fln_4,fw_4),legendtext = plot.legend)
qqcomp(list(fn_4,fln_4,fw_4),legendtext = plot.legend)
cdfcomp(list(fn_4,fln_4,fw_4),legendtext = plot.legend)
ppcomp(list(fn_4,fln_4,fw_4),legendtext = plot.legend)

par(mfrow = c(2,2))
plot.legend = c("GAMMA","CAUCHY","LOGISTIC")
par(mar=c(1,1,1,1))
denscomp(list(fg_4,fc_4,fls_4),legendtext = plot.legend)
qqcomp(list(fg_4,fc_4,fls_4),legendtext = plot.legend)
cdfcomp(list(fg_4,fc_4,fls_4),legendtext = plot.legend)
ppcomp(list(fg_4,fc_4,fls_4),legendtext = plot.legend)

par(mfrow = c(2,2))
plot.legend = c("NORMAL","LOGNORMAL","WEIBULL","GAMMA","CAUCHY","LOGISTIC")
par(mar=c(1,1,1,1))
denscomp(list(fn_4,fln_4,fw_4,fg_4,fc_4,fls_4),legendtext = plot.legend)
qqcomp(list(fn_4,fln_4,fw_4,fg_4,fc_4,fls_4),legendtext = plot.legend)
cdfcomp(list(fn_4,fln_4,fw_4,fg_4,fc_4,fls_4),legendtext = plot.legend)
ppcomp(list(fn_4,fln_4,fw_4,fg_4,fc_4,fls_4),legendtext = plot.legend)

gofstat(list(fn_4,fln_4,fw_4,fg_4,fc_4,fls_4)) # GOODNESS OF FIT METRICS
############### overall_score ATTRIBUTE FOLLOWS WEIBULL DISTRIBUTION ###############

############### ATTRIBUTE-5 --> goal_1_score (NO POVERTY) ###############
summary(data$goal_1_score)
############### IMPUTATION BY MEDIAN ###############
imputed_data_1 = as.numeric(impute(data$goal_1_score,median))
imputed_data_1
data$goal_1_score = imputed_data_1
new_goal_1_score = data.frame(imputed_data_1)
new_goal_1_score

head(new_goal_1_score)

summary(new_goal_1_score)

basicStats(new_goal_1_score)

descriptives(
  data = new_goal_1_score,
  vars = "imputed_data_1",
  freq = TRUE,
  hist = TRUE,
  dens = TRUE,
  bar = FALSE,
  barCounts = FALSE,
  box = FALSE,
  sd = TRUE,
  variance = TRUE,
  range = TRUE,
  se = TRUE,
  skew = TRUE,
  kurt = TRUE,
  pcEqGr = TRUE,
  pcNEqGr = 10)

boxplot(new_goal_1_score,main="BOXPLOT OF goal_1_score",col="orange")

plot(data$overall_score,imputed_data_1,main="SCATTERPLOT",xlab = "overall_score",ylab = "goal_1_score",col="orange",pch=1)

cor(data$overall_score,imputed_data_1)

fn_5 = fitdist(imputed_data_1,"norm") # NORMAL DISTRIBUTION
denscomp(fn_5)
cdfcomp(fn_5)
qqcomp(fn_5)
ppcomp(fn_5)
summary(fn_5)
fln_5 = fitdist(imputed_data_1,"lnorm") # LOGNORMAL DISTRIBUTION
denscomp(fln_5)
cdfcomp(fln_5)
qqcomp(fln_5)
ppcomp(fln_5)
summary(fln_5)
fw_5 = fitdist(imputed_data_1,"weibull") # WEIBULL DISTRIBUTION
denscomp(fw_5)
cdfcomp(fw_5)
qqcomp(fw_5)
ppcomp(fw_5)
summary(fw_5)
fg_5 = fitdist(imputed_data_1,"gamma") # GAMMA DISTRIBUTION
denscomp(fg_5)
cdfcomp(fg_5)
qqcomp(fg_5)
ppcomp(fg_5)
summary(fg_5)
fc_5 = fitdist(imputed_data_1,"cauchy") # CAUCHY DISTRIBUTION
denscomp(fc_5)
cdfcomp(fc_5)
qqcomp(fc_5)
ppcomp(fc_5)
summary(fc_5)
fls_5 = fitdist(imputed_data_1,"logis") # LOGISTIC DISTRIBUTION
denscomp(fls_5)
cdfcomp(fls_5)
qqcomp(fls_5)
ppcomp(fls_5)
summary(fls_5)

############### COMPARISON ###############
par(mfrow = c(2,2))
plot.legend = c("NORMAL","LOGNORMAL","WEIBULL")
par(mar=c(1,1,1,1))
denscomp(list(fn_5,fln_5,fw_5),legendtext = plot.legend)
qqcomp(list(fn_5,fln_5,fw_5),legendtext = plot.legend)
cdfcomp(list(fn_5,fln_5,fw_5),legendtext = plot.legend)
ppcomp(list(fn_5,fln_5,fw_5),legendtext = plot.legend)

par(mfrow = c(2,2))
plot.legend = c("GAMMA","CAUCHY","LOGISTIC")
par(mar=c(1,1,1,1))
denscomp(list(fg_5,fc_5,fls_5),legendtext = plot.legend)
qqcomp(list(fg_5,fc_5,fls_5),legendtext = plot.legend)
cdfcomp(list(fg_5,fc_5,fls_5),legendtext = plot.legend)
ppcomp(list(fg_5,fc_5,fls_5),legendtext = plot.legend)

par(mfrow = c(2,2))
plot.legend = c("NORMAL","LOGNORMAL","WEIBULL","GAMMA","CAUCHY","LOGISTIC")
par(mar=c(1,1,1,1))
denscomp(list(fn_5,fln_5,fw_5,fg_5,fc_5,fls_5),legendtext = plot.legend)
qqcomp(list(fn_5,fln_5,fw_5,fg_5,fc_5,fls_5),legendtext = plot.legend)
cdfcomp(list(fn_5,fln_5,fw_5,fg_5,fc_5,fls_5),legendtext = plot.legend)
ppcomp(list(fn_5,fln_5,fw_5,fg_5,fc_5,fls_5),legendtext = plot.legend)

gofstat(list(fn_5,fln_5,fw_5,fg_5,fc_5,fls_5)) # GOODNESS OF FIT METRICS
############### goal_1_score ATTRIBUTE FOLLOWS CAUCHY DISTRIBUTION ###############

############### ATTRIBUTE-6 --> goal_2_score (ZERO HUNGER) ###############
head(data$goal_2_score)
summary(data$goal_2_score)
plot(data$overall_score,data$goal_2_score,main="SCATTERPLOT",xlab = "overall_score",ylab="goal_2_score",col="red",pch=2)
cor(data$overall_score,data$goal_2_score)
boxplot(data$goal_2_score,main="BOXPLOT OF goal_2_score",col="red")
############### OUTLIERS REMOVAL ###############
outliers_detection_6_1 = boxplot.stats(data$goal_2_score)$out
mtext(paste("OUTLIERS :",paste(outliers_detection_6_1,collapse = ",")))
outliers_removed_6_1 = data$goal_2_score[-which(data$goal_2_score %in% outliers_detection_6_1)]
boxplot(outliers_removed_6_1,main="BOXPLOT OF goal_2_score",col="red")

outliers_detection_6_2 = boxplot.stats(outliers_removed_6_1)$out
mtext(paste("OUTLIERS :",paste(outliers_detection_6_2,collapse = ",")))
outliers_removed_6_2 = outliers_removed_6_1[-which(outliers_removed_6_1 %in% outliers_detection_6_2)]
boxplot(outliers_removed_6_2,main="BOXPLOT OF goal_2_score",col="red")

summary(outliers_removed_6_2)

basicStats(outliers_removed_6_2)

outliers_removed_6_2_df = data.frame(outliers_removed_6_2)

descriptives(
  data = outliers_removed_6_2_df,
  vars = "outliers_removed_6_2",
  freq = TRUE,
  hist = TRUE,
  dens = TRUE,
  bar = FALSE,
  barCounts = FALSE,
  box = FALSE,
  sd = TRUE,
  variance = TRUE,
  range = TRUE,
  se = TRUE,
  skew = TRUE,
  kurt = TRUE,
  pcEqGr = TRUE,
  pcNEqGr = 10)

new_goal_2_score = outliers_removed_6_2
new_goal_2_score

